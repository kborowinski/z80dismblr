{
  "name": "z80dismblr",
  "version": "1.4.1",
  "publisher": "maziac",
  "description": "A Z80 disassembler.",
  "main": "./out/main.js",
  "scripts": {
    "prepublish": "tsc -p ./src",
    "compile": "tsc -p ./src",
    "tslint": "tslint ./src/**/*.ts",
    "watch": "tsc -w -p ./src",
    "test": "mocha -u tdd ./out/tests/",
    "package": "vsce package",
    "publish": "vsce publish",
    "build-docs": "typedoc --out ./out/typedoc --target ES6 --module commonjs --exclude \"**/tests/**/*\" ./src"
  },
  "author": {
    "name": "Thomas Busse"
  },
  "license": "MIT",
  "dependencies": {
    "typescript": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maziac/z80dismblr"
  },
  "devDependencies": {
    "@types/node": "7.0.55",
    "@types/mocha": "2.2.48",
    "mocha": "5.0.1",
    "vscode": "1.1.10",
    "tslint": "5.9.1",
    "vsce": "1.37.5"
  }
}
